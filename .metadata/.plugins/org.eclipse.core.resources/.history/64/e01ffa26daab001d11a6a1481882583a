
public class MultiplosDe3 {
	
	public static void main(String[] args) {
		for(int contador = 0; contador <= 100; contador++) {
			if(contador % 3 == 0) {
				System.out.print(contador);
				System.out.print(" ");
			}
			for(int linha = 0; linha < 6; linha++) {
				System.out.println();
			}
		}
	}

}
